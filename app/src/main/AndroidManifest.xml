<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mlabs.bbm.iquiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="iQuiz"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">

        <activity
            android:name="com.mlabs.bbm.iquiz.SplashScreen"
            android:label="iQuiz">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
             </intent-filter>
        </activity>

        <activity
            android:name="com.mlabs.bbm.iquiz.loginactivity"
            android:label="iQuiz">
            <intent-filter>
                <action android:name="com.mlabs.bbm.firstandroidapp.loginactivity"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mlabs.bbm.iquiz.MainActivity"
            android:label="iQuiz">
            <intent-filter>
                <action android:name="com.mlabs.bbm.firstandroidapp.MainActivity"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mlabs.bbm.iquiz.On_Touch"
            android:label="iQuiz">
            <intent-filter>
                <action android:name="com.mlabs.bbm.firstandroidapp.On_Touch"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mlabs.bbm.iquiz.registeractivity"
            android:label="iQuiz">
            <intent-filter>
                <action android:name="com.mlabs.bbm.firstandroidapp.On_Touch"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>